<?xml version="1.0" encoding="utf-8"?>
<TableLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/faction_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/md_white_1000"
    android:padding="@dimen/padding_8dp">

    <include
        android:id="@+id/row_header"
        layout="@layout/table_row_match_stats_header"/>

    <include
        android:id="@+id/row_1"
        layout="@layout/table_row_match_stats_player"/>

    <include
        android:id="@+id/row_2"
        layout="@layout/table_row_match_stats_player"/>

    <include
        android:id="@+id/row_3"
        layout="@layout/table_row_match_stats_player"/>

    <include
        android:id="@+id/row_4"
        layout="@layout/table_row_match_stats_player"/>

    <include
        android:id="@+id/row_5"
        layout="@layout/table_row_match_stats_player"/>

    <include
        android:id="@+id/row_footer"
        layout="@layout/table_row_match_stats_footer"/>

</TableLayout>